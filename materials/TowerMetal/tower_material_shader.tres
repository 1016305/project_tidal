// TextureParameterTriplanar:8
	vec4 n_out8p0 = triplanar_texture(Material2Metallic, triplanar_power_normal, triplanar_pos);


// Mix:13
	vec4 n_out13p0 = mix(n_out4p0, n_out8p0, n_out14p0);


// TextureParameterTriplanar:5
	vec4 n_out5p0 = triplanar_texture(Material1Roughness, triplanar_power_normal, triplanar_pos);


// TextureParameterTriplanar:7
	vec4 n_out7p0 = triplanar_texture(Material2Roughness, triplanar_power_normal, triplanar_pos);


// Mix:12
	vec4 n_out12p0 = mix(n_out5p0, n_out7p0, n_out14p0);


// TextureParameterTriplanar:3
	vec4 n_out3p0 = triplanar_texture(Material1Normal, triplanar_power_normal, triplanar_pos);


// TextureParameterTriplanar:9
	vec4 n_out9p0 = triplanar_texture(Material2Normal, triplanar_power_normal, triplanar_pos);


// Mix:11
	vec4 n_out11p0 = mix(n_out3p0, n_out9p0, n_out14p0);
	float n_out11p1 = n_out11p0.r;
	float n_out11p2 = n_out11p0.g;
	float n_out11p3 = n_out11p0.b;
	float n_out11p4 = n_out11p0.a;


// VectorCompose:17
	vec4 n_out17p0 = vec4(n_out11p1, n_out11p2, n_out11p3, n_out11p4);


// Output:0
	ALBEDO = vec3(n_out10p0.xyz);
	METALLIC = n_out13p0.x;
	ROUGHNESS = n_out12p0.x;
	NORMAL_MAP = vec3(n_out17p0.xyz);


}
