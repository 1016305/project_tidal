// Texture2D:25
	n_out25p0 = texture(SandMask, UV);


// Reroute:27
	vec4 n_out27p0 = n_out25p0;


// Mix:17
	vec4 n_out17p0 = mix(n_out16p0, n_out18p0, n_out27p0);


// TextureParameterTriplanar:4
	vec4 n_out4p0 = triplanar_texture(GrassMetallic, triplanar_power_normal, triplanar_pos);


// TextureParameterTriplanar:8
	vec4 n_out8p0 = triplanar_texture(StoneMetallic, triplanar_power_normal, triplanar_pos);


// Mix:12
	vec4 n_out12p0 = mix(n_out4p0, n_out8p0, n_out13p0);


// TextureParameterTriplanar:19
	vec4 n_out19p0 = triplanar_texture(SandMetallic, triplanar_power_normal, triplanar_pos);


// Mix:22
	vec4 n_out22p0 = mix(n_out12p0, n_out19p0, n_out27p0);


// TextureParameterTriplanar:5
	vec4 n_out5p0 = triplanar_texture(GrassRoughness, triplanar_power_normal, triplanar_pos);


// TextureParameterTriplanar:7
	vec4 n_out7p0 = triplanar_texture(StoneRoughness, triplanar_power_normal, triplanar_pos);


// Mix:11
	vec4 n_out11p0 = mix(n_out5p0, n_out7p0, n_out13p0);


// TextureParameterTriplanar:20
	vec4 n_out20p0 = triplanar_texture(SandRoughness, triplanar_power_normal, triplanar_pos);


// Mix:23
	vec4 n_out23p0 = mix(n_out11p0, n_out20p0, n_out27p0);


// TextureParameterTriplanar:3
	vec4 n_out3p0 = triplanar_texture(GrassNormal, triplanar_power_normal, triplanar_pos);


// TextureParameterTriplanar:9
	vec4 n_out9p0 = triplanar_texture(StoneNormal, triplanar_power_normal, triplanar_pos);


// Mix:10
	vec4 n_out10p0 = mix(n_out3p0, n_out9p0, n_out13p0);


// TextureParameterTriplanar:21
	vec4 n_out21p0 = triplanar_texture(SandNormal, triplanar_power_normal, triplanar_pos);


// Mix:24
	vec4 n_out24p0 = mix(n_out10p0, n_out21p0, n_out27p0);


// Output:0
	ALBEDO = vec3(n_out17p0.xyz);
	METALLIC = n_out22p0.x;
	ROUGHNESS = n_out23p0.x;
	NORMAL = vec3(n_out24p0.xyz);


}
