// Mix:12
	vec4 n_out12p0 = mix(n_out3p0, n_out9p0, n_out14p0);


// TextureParameterTriplanar:5
	vec4 n_out5p0 = triplanar_texture(Metal1_Rough, triplanar_power_normal, triplanar_pos);


// TextureParameterTriplanar:10
	vec4 n_out10p0 = triplanar_texture(Metal2_Rough, triplanar_power_normal, triplanar_pos);


